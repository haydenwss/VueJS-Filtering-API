levation=\"2\">\n          <v-card-title>{{ result.name }} </v-card-title>\n          <v-divider class=\"mx-4\"></v-divider>\n          \n          <v-card-text>\n            \n            <div class=\"textbox\" v-if=\"showText\">\n              <v-chip-group\n                v-model=\"selection\"\n                active-class=\"deep-purple accent-4 white--text\"\n                column \n              >\n                <v-chip close color=\"orange\" label outlined>{{ result.gender }}</v-chip>\n                <v-chip close color=\"orange\" label outlined>{{ result.house }}</v-chip>\n                <v-chip close color=\"orange\" label outlined>Student: {{ result.hogwartsStudent }}</v-chip>\n                <v-chip close color=\"orange\" label outlined>Staff: {{ result.hogwartsStaff }}</v-chip>\n                <v-chip close color=\"orange\" label outlined>{{ result.wand }}</v-chip>\n                <v-chip close color=\"orange\" label outlined>{{ result.yearOfBirth }}</v-chip>\n              </v-chip-group>\n            </div>\n          </v-card-text>\n        </v-card>\n      </div>    \n  </v-container>\n</template>\n\n<script>\nimport axios from \"axios\";\n\nconst CHARACTERS_API_ENDPOINT = \"http://hp-api.herokuapp.com/api/characters/\";\n\n  export default {\n    name: 'HelloWorld',\n    data() {\n      return {\n        results:undefined,\n        search: \"\",\n        showText: false\n      }\n    },\n    methods: {\n\n    },\n       \n    created() {\n      axios\n      .get(CHARACTERS_API_ENDPOINT)\n        .then(response => {\n          this.results=response.data;\n        });        \n    },\n   \n    // Here is where the code breaks down \n    // It needs an else statement, to reload the original API data\n    // This can manually be done on line 18 by reloading the ablove ^^ this.results\n\n    computed: {\n      filteredResults: function() {\n        return this.results.filter((result) => {\n          return result.name.match(this.search) \n        }) \n      }\n    }\n  }\n\n\n</script>\n\n<style>\n\n.main--container {\n  border-style: solid;\n  padding: 20px;\n  margin: 40px;\n  margin-top: 250px;\n  background-color: rgb(16, 25, 46);\n  color: white;\n}\n\nsection {\n  background: url('https://images.unsplash.com/photo-1504251212767-f12d0fa9eb5e?ixid=MXwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHw%3D&ixlib=rb-1.2.1&auto=format&fit=crop&w=1400&q=80');\n  height: 300px;\n  padding: 10%;\n}\n\n</style>\n\n\n"]}]}